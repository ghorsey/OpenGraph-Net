{
  "version": "1.3.4-*",
  "buildOptions": {
    "keyFile": "OpenGraph-Net.snk",
    "warningsAsErrors": true,
    "xmlDoc": true
  },
  "dependencies": {
    "HtmlAgilityPack": "1.4.9.5",
  },
  "copyright": "Copyright © 2016",
  "title":  "OpenGraph.Net", 
  "authors": [ "ghorsey" ], 
  "packOptions": {
    "projectUrl": "https://github.com/ghorsey/OpenGraph-Net",
    "summary": "A simple parser for the OpenGraph protocol (http://ogp.me)",
    "releaseNotes": "Added async parse url methods",
    "tags": [ "OpenGraph" ],
    "licenseUrl": "http://opensource.org/licenses/MIT",
    "owners": [ "ghorsey" ],
    "repository": {
      "type": "git",
      "url": "https://github.com/ghorsey/OpenGraph-Net"
    }
  },
  "frameworks": {
    "net45": {
      "frameworkAssemblies": {
        "System.Xml":  "4.0.0" 
      }
    },
    "net451": {
      "frameworkAssemblies": {
        "System.Xml":  "4.0.0" 
      }
    },
    "net46": {
      "frameworkAssemblies": {
        "System.Xml":  "4.0.0" 
      }
    },
    "net461": {
      "frameworkAssemblies": {
        "System.Xml":  "4.0.0" 
      }
    },
    "netstandard1.6": {
      "buildOptions": {
        "define": [
          "NETSTANDARD16"
        ]
      },
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.IO.Compression": "4.1.0",
        "System.Net.Http": "4.1.0",
        "System.Runtime.Serialization.Formatters": "4.0.0-rc3-24212-01",
        "System.Runtime.InteropServices": "4.1.0",
        "System.Net.Requests": "4.0.11"
      },
      "imports": [
        "dnxcore50",
        "dotnet5.6",
        "portable-net45+win"
      ]
    }
  }
}
